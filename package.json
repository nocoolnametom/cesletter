{
  "name": "cesletter",
  "version": "1.2.2",
  "description": "Provides the Letter to a CES Director in a variety of useful formats",
  "main": "dist/js/export.js",
  "style": "dist/css/cesletter.css",
  "jsnext:main": "src/js/export.js",
  "scripts": {
    "build": "npm run copy-images && npm run build-css && npm run build-html && npm run build-html-offline && npm run rollup && npm run rollup-export",
    "build-css": "node-sass --output-style compressed ./src/scss/cesletter.scss ./dist/css/cesletter.css",
    "build-html": "babel-node build/build_md.js",
    "build-html-offline": "babel-node build/build_offline.js",
    "copy-images": "mkdir -p ./dist/img ; cp src/img/* ./dist/img/",
    "offline": "npm run offline-files; npm run offline-videos; npm run offline-simple; npm run offline-complex",
    "offline-complex": "babel-node ./build/save_offline_complex_sites.js",
    "offline-files": "babel-node ./build/save_offline_files.js",
    "offline-simple": "babel-node ./build/save_offline_simple_sites.js",
    "offline-videos": "babel-node ./build/save_offline_videos.js",
    "prepublish": "npm run build",
    "rollup": "rollup --config config/rollup.config.js",
    "rollup-export": "rollup --config config/rollup.export.js",
    "rollup-watch": "rollup -w --config config/rollup.config.js",
    "start": "http-server dist/ -s -o -p 8886 ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nocoolnametom/cesletter.git"
  },
  "keywords": [
    "cesletter",
    "mormon",
    "lds"
  ],
  "author": "Tom Doggett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nocoolnametom/cesletter/issues"
  },
  "homepage": "https://github.com/nocoolnametom/cesletter#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^13.1.0",
    "crypto": "0.0.3",
    "domready": "^1.0.8",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "finalhandler": "^0.5.0",
    "handlebars": "^4.0.5",
    "html-minifier": "^3.0.2",
    "http-mitm-proxy": "^0.5.1",
    "markdown-it": "^7.0.1",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-block-image": "0.0.3",
    "markdown-it-center-text": "^1.0.3",
    "markdown-it-container": "^2.0.0",
    "markdown-it-decorate": "^1.2.1",
    "markdown-it-front-matter": "^0.1.2",
    "markdown-it-include": "0.0.2",
    "markdown-it-link-attributes": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "mdfigcaption": "^0.1.1",
    "node-forge": "^0.6.42",
    "node-sass": "^4.14.1",
    "phantom": "^2.1.21",
    "phantomjs-prebuilt": "^2.1.12",
    "q": "^2.0.3",
    "rollup": "^0.35.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-filesize": "^0.5.5",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "serve-static": "^1.11.1"
  },
  "dependencies": {
    "http-server": "^0.9.0"
  }
}
