{
  "name": "cesletter",
  "version": "1.0.0",
  "description": "Provides the Letter to a CES Director in a variety of useful formats",
  "main": "index.js",
  "scripts": {
    "build": "npm run copy-images && npm run build-css && npm run build-html",
    "build-css": "node-sass ./src/scss/cesletter.scss ./dist/css/cesletter.css",
    "build-html": "babel-node ./build.js",
    "build-js": "mkdir -p ./dist/js; browserify ./src/js/cesletter.js > ./dist/js/cesletter.js",
    "copy-images": "mkdir -p ./dist/img ; cp src/img/* ./dist/img/",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nocoolnametom/cesletter.git"
  },
  "keywords": [
    "cesletter",
    "mormon",
    "lds"
  ],
  "author": "Tom Doggett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nocoolnametom/cesletter/issues"
  },
  "homepage": "https://github.com/nocoolnametom/cesletter#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^13.1.0",
    "crypto": "0.0.3",
    "domready": "^1.0.8",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "finalhandler": "^0.5.0",
    "handlebars": "^4.0.5",
    "http-mitm-proxy": "^0.5.1",
    "markdown-it": "^7.0.1",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-block-image": "0.0.3",
    "markdown-it-center-text": "^1.0.3",
    "markdown-it-container": "^2.0.0",
    "markdown-it-decorate": "^1.2.1",
    "markdown-it-front-matter": "^0.1.2",
    "markdown-it-include": "0.0.2",
    "markdown-it-link-attributes": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "mdfigcaption": "^0.1.1",
    "node-forge": "^0.6.42",
    "node-sass": "^3.8.0",
    "phantom": "^2.1.21",
    "phantomjs-prebuilt": "^2.1.12",
    "q": "^2.0.3",
    "serve-static": "^1.11.1"
  }
}
